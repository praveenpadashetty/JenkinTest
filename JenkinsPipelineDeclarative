pipeline{
agent any
stages{
        stage('checkout'){
	    steps{
            git 'https://github.com/praveenpadashetty/JenkinTest.git'
	}
             
        }
         
         stage('build'){
	steps{
             echo 'I am in built'
	}
        }
         stage('archive'){
	steps{
             echo 'I am archived'
	}
        }
}
}